    <div class="main-container-creategroup">
        <div class="main-side-creategroup">
            <h1>Create your Group</h1>

            <form action="/group/creategroup" method="POST" id="form-creategroup">
                <label for="groupname">Group name</label>
                <input id="groupname" type="text" name="groupname" placeholder="Insert your groupname">

                <label for="description">Description</label>
                <input contenteditable="true" id="description" type="text" name="description" placeholder="Describe your group">
            
                <input id="adminId" type="text"  name="adminId" style="display: none" value="<%=userId%>">
                <input type ="submit">
            </form>
        </div>
        <div class="footer-side">
